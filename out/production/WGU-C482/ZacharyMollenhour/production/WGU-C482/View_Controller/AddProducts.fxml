<AnchorPane id="AnchorPane" prefHeight="586.0" prefWidth="846.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.AddProductsController">
 <children>
