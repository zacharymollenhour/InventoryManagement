      <Label layoutX="15.0" layoutY="143.0" text="Inv" />
      <Label layoutX="15.0" layoutY="179.0" text="Price" />
      <Label layoutX="15.0" layoutY="215.0" text="Min" />
      <Label layoutX="15.0" layoutY="251.0" text="Max" />
